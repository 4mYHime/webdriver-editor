{
    "manifest_version": 2,
    "name": "Navigation Cleaner",
    "version": "1.0",
    "description": "Remove Navigation.Webdriver",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "permissions": [
        "http://api.flickr.com/"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["myscript.js"],
            "run_at": "document_start"
        }
    ]
}
